{
  "name": "@dinktech/tw-designer-cli",
  "version": "1.0.0",
  "description": "a CLI tool to help extend TailwindCSS with a user-defined design system.",
  "main": "lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "watch": "rm -rf lib && tsc --watch",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "run": "tsc -p . && node ./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wahbadb94/tw-themer-cli.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css",
    "cli",
    "codegen",
    "theme",
    "design",
    "design-system"
  ],
  "author": "Dustin Wahba",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wahbadb94/tw-themer-cli/issues"
  },
  "homepage": "https://github.com/wahbadb94/tw-themer-cli#readme",
  "bin": {
    "tw-designer": "lib/index.js"
  },
  "devDependencies": {
    "@types/clear": "^0.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.18",
    "@types/prompt-sync": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.20",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "clear": "^0.1.0",
    "commander": "^9.5.0",
    "inquirer": "^9.1.4",
    "prompt-sync": "^4.2.0"
  }
}
